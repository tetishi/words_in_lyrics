p header_image_url:
= @header_image_url

p song_art_image_thumbnail_url:
= @song_art_image_thumbnail_url

p song_art_image_url:
= @song_art_image_url

p request paramter:
= @query

p title
= @title

p title_with_featured
= @title_with_featured

p result:
= @result

p message:
= @message
